#
#	PCF file for the iCE40 Ultra Plus FPGA (uwg30 package) on MDP board
#	On the MDP board, pin D3 of the FPGA is connected to LED D14.
#

set_io led_o[7] E4 #9B
set_io led_o[6] D5 #2A
set_io led_o[5] E5 #0A
set_io led_o[4] C5 #11B_G5
set_io led_o[3] B5 #13B
set_io led_o[2] F2 #25B_G3
set_io led_o[1] A5 #24A
set_io led_o[0] D3 #10A

